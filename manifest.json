{
	"name": "__MSG_extName__",
  "default_locale": "en",
	"version": "0.9",
	"manifest_version" : 2,
	"description": "__MSG_extDescription__",
	"background" : {
    "scripts": ["scripts/background.js"]
	},
  "permissions" : [
    "contextMenus",
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*"
  ],
	"page_action" : {
		"default_icon": {
			"16": "icons/16.png",
			"19": "icons/19.png",
			"38": "icons/38.png",
			"48": "icons/48.png",
			"128": "icons/128.png"
		},
		"default_title" : "__MSG_extName__"
	},
	"content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["lib/jquery-2.0.3.min.js","lib/jquery.exif.js","scripts/main.js"]
    }
	],
  "minimum_chrome_version" : "16.0.0.0",
  "web_accessible_resources": ["lib/jquery-2.0.3.min.map"],
	"icons": {
		"16": "icons/16.png",
		"48": "icons/48.png",
		"128": "icons/128.png"
	},
	"options_page" : "options.html"
}
